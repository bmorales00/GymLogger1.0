<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="wrap_content"
                                             android:padding="10dp" android:layout_margin="1dp"
                                             app:layout_insetEdge="none"
                                             android:background="@color/white">
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/tvNameOfExc"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:outlineProvider="none" android:singleLine="false" android:clickable="true"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" android:textAllCaps="true"
            android:textSize="22sp"
            android:minHeight="24sp" android:fontFamily="sans-serif-black"/>
</android.support.constraint.ConstraintLayout>